@model IEnumerable<PersonalSite.Models.Snippet>

@{
    ViewBag.Title = "Swift";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Swift Snippets</h2>

@foreach (var snippet in Model)
{
    <div class="snippetLink">
        @Html.ActionLink(snippet.Title, "ViewSnippet", new { id = snippet.ID })
    </div>
}
